<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lael.infra.modules.codegroup.CodeGroupMapper">

    <resultMap id="resultMapObj" type="com.lael.infra.modules.codegroup.CodeGroup"></resultMap>
	
	<select id="selectList" resultMap="resultMapObj">
		SELECT
			a.*
			,(select count(b.codeGroup_ccgSeq) from code b where 1=1 and a.ccgSeq=b.codeGroup_ccgSeq) as count
		FROM 
			codeGroup a
		<!-- WHERE 1=1
			AND ifcgDelNy = 0 -->
	</select>

</mapper>	